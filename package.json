{
  "version": "2.0.0",
  "scripts": {
    "build-server": "cd server && tsc",
    "start-server": "cd server && node ./dist/index.js",
    "build-client": "cd client && npm run build",
    "start-client": "cd client && npm run preview -- --host --port 4455",
    "build": "npm run build-server && npm run build-client",
    "dev": "concurrently \"nodemon ./server/index.ts\" \"cd client && npm run dev -- --host --port 4455\"",
    "start": "concurrently \"npm run start-server\" \"npm run start-client\""
  },
  "dependencies": {
    "@vueuse/head": "^0.7.12",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
  }
}
